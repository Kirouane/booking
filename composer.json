{
  "name": "kirouane/booking",
  "description": "Booking API",
  "type": "project",
  "minimum-stability":"stable",
  "homepage": "https://github.com/Kirouane/booking",
  "license": "MIT",
  "authors": [
    {
      "name": "nassim.kirouane",
      "email": "nassim.kirouane@gmail.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/Kirouane/booking/issues"
  },
  "autoload": {
    "psr-4": {
      "Booking\\": "src/Booking/",
      "Maintenance\\": "src/Maintenance/",
      "Lib\\": "src/Lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Booking\\": "test/unit/Booking/",
      "Lib\\": "test/unit/Lib/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "require": {
    "php" : ">= 7.1",
    "kirouane/interval": "^1.3",
    "zendframework/zend-diactoros": "^1.7",
    "middlewares/request-handler": "^1.1",
    "middlewares/fast-route": "^1.0",
    "middlewares/payload": "^1.0",
    "zendframework/zend-db": "^2.9",
    "ramsey/uuid": "^3.7",
    "zendframework/zend-hydrator": "^2.3",
    "zendframework/zend-filter": "^2.7",
    "zendframework/zend-servicemanager": "^3.3",
    "php-amqplib/php-amqplib": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5@dev",
    "mockery/mockery": "^1.0@dev",
    "phpro/grumphp": "dev-master",
    "php-coveralls/php-coveralls": "^2.0@dev",
    "friendsofphp/php-cs-fixer": "^2.9@dev",
    "jakub-onderka/php-parallel-lint": "dev-master",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "maglnet/composer-require-checker": "^1.0@dev",
    "phpmd/phpmd": "dev-master",
    "phpstan/phpstan": "^0.9.0@dev",
    "sebastian/phpcpd": "^3.0@dev",
    "infection/infection": "dev-master",
    "povils/phpmnd": "^1.1@dev",
    "sensiolabs/security-checker": "^5.0@dev"
  }
}
